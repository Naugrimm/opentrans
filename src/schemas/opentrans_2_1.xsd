    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ACCOUNTING_PERIOD">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ACCOUNTING_PERIOD_START_DATE"/>
        <xsd:element ref="ACCOUNTING_PERIOD_END_DATE"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ACCOUNTING_PERIOD_END_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="ACCOUNTING_PERIOD_START_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="ADDRESS">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:NAME" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:NAME2" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:NAME3" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:DEPARTMENT" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="CONTACT_DETAILS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:STREET" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:ZIP" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:BOXNO" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:ZIPBOX" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:CITY" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:STATE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:COUNTRY" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:COUNTRY_CODED" minOccurs="0"/>
        <xsd:element ref="bmecat:VAT_ID" minOccurs="0"/>
        <xsd:element ref="TAX_NUMBER" minOccurs="0"/>
        <xsd:element ref="bmecat:PHONE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:FAX" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
          <xsd:element ref="bmecat:EMAIL"/>
          <xsd:element ref="bmecat:PUBLIC_KEY" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:element ref="bmecat:URL" minOccurs="0"/>
        <xsd:element ref="bmecat:ADDRESS_REMARKS" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ADJUSTED_INVOICE_SUMMARY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="NET_VALUE_GOODS"/>
        <xsd:element ref="TOTAL_TAX"/>
        <xsd:element ref="TOTAL_AMOUNT"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ADJUSTED_SUMMARY_AMOUNT" type="bmecat:dtNUMBER"/>
  <xsd:element name="ADJUSTMENT_REASON_CODE" type="dtADJUSTMENT_REASON_CODES"/>
  <xsd:element name="ADJUSTMENT_REASON_DESCR" type="bmecat:dtMLSTRING"/>
  <xsd:element name="AGREEMENT">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:AGREEMENT_ID"/>
        <xsd:element ref="bmecat:AGREEMENT_LINE_ID" minOccurs="0"/>
        <xsd:element ref="bmecat:AGREEMENT_START_DATE" minOccurs="0"/>
        <xsd:element ref="bmecat:AGREEMENT_END_DATE"/>
        <xsd:element ref="bmecat:SUPPLIER_IDREF" minOccurs="0"/>
        <xsd:element ref="AGREEMENT_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="type" use="optional">
        <xsd:simpleType>
          <xsd:restriction base="bmecat:dtSTRING">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="50"/>
            <xsd:pattern value="buyer|supplier|\w{1,50}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
      <xsd:attribute name="default" type="dtBOOLEAN" use="optional"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AGREEMENT_DESCR">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:restriction base="bmecat:dtMLSTRING">
          <xsd:maxLength value="250"/>
          <xsd:minLength value="1"/>
        </xsd:restriction>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ALLOW_OR_CHARGE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ALLOW_OR_CHARGE_SEQUENCE" minOccurs="0"/>
        <xsd:element ref="ALLOW_OR_CHARGE_NAME" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ALLOW_OR_CHARGE_TYPE" minOccurs="0"/>
        <xsd:element ref="ALLOW_OR_CHARGE_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ALLOW_OR_CHARGE_VALUE" minOccurs="0"/>
        <xsd:element ref="ALLOW_OR_CHARGE_BASE" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="type" use="required">
        <xsd:simpleType>
          <xsd:restriction base="bmecat:dtSTRING">
            <xsd:enumeration value="allowance"/>
            <xsd:enumeration value="surcharge"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ALLOW_OR_CHARGE_AMOUNT" type="bmecat:dtFLOAT"/>
  <xsd:element name="ALLOW_OR_CHARGE_BASE" type="bmecat:dtFLOAT"/>
  <xsd:element name="ALLOW_OR_CHARGE_DESCR">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:restriction base="bmecat:dtMLSTRING">
          <xsd:minLength value="1"/>
          <xsd:maxLength value="250"/>
        </xsd:restriction>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ALLOW_OR_CHARGE_NAME">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:restriction base="bmecat:dtMLSTRING">
          <xsd:minLength value="1"/>
          <xsd:maxLength value="80"/>
        </xsd:restriction>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ALLOW_OR_CHARGE_SEQUENCE" type="bmecat:dtCOUNT"/>
  <xsd:element name="ALLOW_OR_CHARGE_TYPE">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="30"/>
        <xsd:minLength value="1"/>
        <xsd:pattern
            value="abroad|administration|bulk_goods|cash_discount|charge|cod|customs|customization|deposit|express|freight|handling|insurance|island|material|packing|partial_quantity|period_bonus|postage|project_bonus|overpackaging|rebate|recycling|small_order|special_work_times|toll|[\w\-\.]{1,30}"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ALLOW_OR_CHARGE_VALUE">
    <xsd:complexType>
      <xsd:choice>
        <xsd:element ref="AOC_PERCENTAGE_FACTOR"/>
        <xsd:element ref="AOC_MONETARY_AMOUNT"/>
        <xsd:element ref="AOC_ORDER_UNITS_COUNT"/>
        <xsd:element ref="AOC_ADDITIONAL_ITEMS"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ALLOW_OR_CHARGES">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ALLOW_OR_CHARGE" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ALLOW_OR_CHARGES_FIX">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ALLOW_OR_CHARGE" maxOccurs="unbounded"/>
        <xsd:element ref="ALLOW_OR_CHARGES_TOTAL_AMOUNT" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ALLOW_OR_CHARGES_TOTAL_AMOUNT" type="bmecat:dtFLOAT"/>
  <xsd:element name="ALT_CUSTOMER_ORDER_ID">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="250"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AMOUNT_OVER_CREDIT_LIMIT" type="bmecat:dtNUMBER"/>
  <xsd:element name="AOC_ADDITIONAL_ITEMS">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="250"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="AOC_MONETARY_AMOUNT" type="bmecat:dtFLOAT"/>
  <xsd:element name="AOC_ORDER_UNITS_COUNT">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="bmecat:dtFLOAT">
          <xsd:attribute name="type" use="required">
            <xsd:simpleType>
              <xsd:restriction base="bmecat:dtSTRING">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="20"/>
                <xsd:enumeration value="inclusive"/>
                <xsd:enumeration value="exclusive"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="AOC_PERCENTAGE_FACTOR" type="bmecat:dtFLOAT"/>
  <xsd:element name="BANK_ACCOUNT">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="typeSTRING00100">
          <xsd:attribute name="type" use="optional">
            <xsd:simpleType>
              <xsd:restriction base="bmecat:dtSTRING">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="20"/>
                <xsd:pattern value="standard|iban|\w{1,20}"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="BANK_CODE">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="typeSTRING00050">
          <xsd:attribute name="type" use="optional">
            <xsd:simpleType>
              <xsd:restriction base="bmecat:dtSTRING">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="20"/>
                <xsd:pattern value="bic|\w{1,20}"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="BANK_COUNTRY" type="bmecat:dtCOUNTRIES"/>
  <xsd:element name="BANK_NAME">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="50"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="BUYER_PARTY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:PARTY"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CARD">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="CARD_NUM"/>
        <xsd:element ref="CARD_AUTH_CODE" minOccurs="0"/>
        <xsd:element ref="CARD_REF_NUM" minOccurs="0"/>
        <xsd:element ref="CARD_EXPIRATION_DATE"/>
        <xsd:element ref="CARD_HOLDER_NAME"/>
      </xsd:sequence>
      <xsd:attribute name="type" use="required">
        <xsd:simpleType>
          <xsd:restriction base="bmecat:dtSTRING">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="50"/>
            <xsd:pattern
                value="MasterCard|VISA|AmericanExpress|DinersClub|JCB|Maestro|DiscoverCard|Transcard|DinaCard|ChinaUnionPay|\w{1,50}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CARD_AUTH_CODE">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="10"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CARD_EXPIRATION_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="CARD_HOLDER_NAME">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="50"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CARD_NUM">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="16"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CARD_REF_NUM">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="50"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CARD_TYPE">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="typeSTRING00100">
          <xsd:attribute name="lang" type="dtLANG" use="optional"/>
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CASH" type="bmecat:dtBOOLEAN"/>
  <xsd:element name="CATALOG_REFERENCE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:CATALOG_ID"/>
        <xsd:element ref="bmecat:CATALOG_VERSION"/>
        <xsd:element ref="bmecat:CATALOG_NAME" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CENTRAL_REGULATION" type="bmecat:dtBOOLEAN" default="false"/>
  <xsd:element name="CHECK" type="bmecat:dtBOOLEAN"/>
  <xsd:element name="CLOSING_BALANCE" type="bmecat:dtNUMBER"/>
  <xsd:element name="CONFIG_CODE_FIX">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="6000"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="CONTACT_DETAILS">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:CONTACT_ID" minOccurs="0"/>
        <xsd:element ref="bmecat:CONTACT_NAME" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:FIRST_NAME" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:TITLE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:ACADEMIC_TITLE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="CONTACT_ROLE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:CONTACT_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:PHONE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:FAX" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:URL" minOccurs="0"/>
        <xsd:element ref="bmecat:EMAILS" minOccurs="0"/>
        <xsd:element ref="bmecat:AUTHENTIFICATION" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CONTACT_ROLE">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="typeMLSTRING00050">
          <xsd:attribute name="type" use="optional">
            <xsd:simpleType>
              <xsd:restriction base="bmecat:dtSTRING">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="20"/>
                <xsd:enumeration value="administrativ"/>
                <xsd:enumeration value="commercial"/>
                <xsd:enumeration value="document_issuer"/>
                <xsd:enumeration value="special_treatment"/>
                <xsd:enumeration value="technical"/>
                <xsd:enumeration value="others"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CONTROL_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="STOP_AUTOMATIC_PROCESSING" minOccurs="0"/>
        <xsd:element ref="GENERATOR_INFO" minOccurs="0"/>
        <xsd:element ref="GENERATION_DATE" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CREDIT_LIMIT" type="bmecat:dtNUMBER"/>
  <xsd:element name="CREDIT_AVAILABLE" type="bmecat:dtNUMBER"/>
  <xsd:element name="CUSTOMER_IDREF" type="bmecat:typePARTY_ID"/>
  <xsd:element name="CUSTOMER_ORDER_REFERENCE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDER_ID" minOccurs="0"/>
        <xsd:element ref="LINE_ITEM_ID" minOccurs="0"/>
        <xsd:element ref="ORDER_DATE" minOccurs="0"/>
        <xsd:element ref="ORDER_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="CUSTOMER_IDREF" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DAYS" type="bmecat:dtCOUNT"/>
  <xsd:element name="DEBIT" type="bmecat:dtBOOLEAN"/>
  <xsd:element name="DELIVERER_IDREF" type="bmecat:typePARTY_ID"/>
  <xsd:element name="DELIVERY_COMPLETED" type="bmecat:dtBOOLEAN"/>
  <xsd:element name="DELIVERY_DATE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DELIVERY_START_DATE"/>
        <xsd:element ref="DELIVERY_END_DATE"/>
      </xsd:sequence>
      <xsd:attribute name="type" use="optional" default="fixed">
        <xsd:simpleType>
          <xsd:restriction base="bmecat:dtSTRING">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="20"/>
            <xsd:enumeration value="optional"/>
            <xsd:enumeration value="fixed"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DELIVERY_END_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="DELIVERY_IDREF" type="bmecat:typePARTY_ID"/>
  <xsd:element name="DELIVERY_PARTY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:PARTY"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DELIVERY_REFERENCE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DELIVERYNOTE_ID" minOccurs="0"/>
        <xsd:element ref="LINE_ITEM_ID" minOccurs="0"/>
        <xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
        <xsd:element ref="DELIVERY_IDREF" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DELIVERY_START_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="DELIVERYNOTE_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="DELIVERYNOTE_ID">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="250"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DIRECT_DEBIT" type="bmecat:dtNUMBER"/>
  <xsd:element name="DISCOUNT_FACTOR" type="bmecat:dtFLOAT"/>
  <xsd:element name="DISPATCHNOTIFICATION">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DISPATCHNOTIFICATION_HEADER"/>
        <xsd:element ref="DISPATCHNOTIFICATION_ITEM_LIST"/>
        <xsd:element ref="DISPATCHNOTIFICATION_SUMMARY"/>
      </xsd:sequence>
      <xsd:attribute name="version" type="typeOPENTRANS_version" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DISPATCHNOTIFICATION_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="DISPATCHNOTIFICATION_HEADER">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="CONTROL_INFO" minOccurs="0"/>
        <xsd:element ref="DISPATCHNOTIFICATION_INFO"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DISPATCHNOTIFICATION_ID">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="250"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="DISPATCHNOTIFICATION_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DISPATCHNOTIFICATION_ID"/>
        <xsd:element ref="DISPATCHNOTIFICATION_DATE" minOccurs="0"/>
        <xsd:element ref="bmecat:LANGUAGE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:MIME_ROOT" minOccurs="0"/>
        <xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
        <xsd:element ref="PARTIES"/>
        <xsd:element ref="bmecat:SUPPLIER_IDREF"/>
        <xsd:element ref="bmecat:BUYER_IDREF" minOccurs="0"/>
        <xsd:element ref="SHIPMENT_PARTIES_REFERENCE"/>
        <xsd:element ref="SHIPMENT_ID" minOccurs="0"/>
        <xsd:element ref="TRACKING_TRACING_URL" minOccurs="0"/>
        <xsd:element ref="DOCEXCHANGE_PARTIES_REFERENCE" minOccurs="0"/>
        <xsd:element ref="LOGISTIC_DETAILS_INFO" minOccurs="0"/>
        <xsd:element ref="bmecat:INTERNATIONAL_RESTRICTIONS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="HEADER_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DISPATCHNOTIFICATION_ITEM">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="LINE_ITEM_ID"/>
        <xsd:element ref="PRODUCT_ID"/>
        <xsd:element ref="PRODUCT_FEATURES" minOccurs="0"/>
        <xsd:element ref="PRODUCT_COMPONENTS" minOccurs="0"/>
        <xsd:element ref="QUANTITY"/>
        <xsd:element ref="bmecat:ORDER_UNIT"/>
        <xsd:element ref="PARTIAL_DELIVERY_LIST" minOccurs="0"/>
        <xsd:element ref="DELIVERY_COMPLETED" minOccurs="0"/>
        <xsd:element ref="DELIVERY_REFERENCE" minOccurs="0"/>
        <xsd:element ref="bmecat:SUPPLIER_IDREF" minOccurs="0"/>
        <xsd:element ref="ORDER_REFERENCE"/>
        <xsd:element ref="SUPPLIER_ORDER_REFERENCE" minOccurs="0"/>
        <xsd:element ref="CUSTOMER_ORDER_REFERENCE" minOccurs="0"/>
        <xsd:element ref="SHIPMENT_PARTIES_REFERENCE"/>
        <xsd:element ref="LOGISTIC_DETAILS" minOccurs="0"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ITEM_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DISPATCHNOTIFICATION_ITEM_LIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DISPATCHNOTIFICATION_ITEM" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DISPATCHNOTIFICATION_SUMMARY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="TOTAL_ITEM_NUM"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DOCEXCHANGE_PARTIES_REFERENCE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DOCUMENT_ISSUER_IDREF" minOccurs="0"/>
        <xsd:element ref="DOCUMENT_RECIPIENT_IDREF" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="DOCUMENT_ISSUER_IDREF" type="bmecat:typePARTY_ID"/>
  <xsd:element name="DOCUMENT_RECIPIENT_IDREF" type="bmecat:typePARTY_ID"/>
  <xsd:element name="E_BILLING">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="INVOICE_ORIGINAL" minOccurs="0"/>
        <xsd:element ref="SIGNATURE_AND_VERIFICATION" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="EXCHANGE_RATE" type="bmecat:dtNUMBER"/>
  <xsd:element name="FEATURE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:choice>
          <xsd:element ref="bmecat:FNAME" maxOccurs="unbounded"/>
          <xsd:element ref="bmecat:FT_IDREF"/>
          <xsd:element ref="bmecat:FTEMPLATE"/>
        </xsd:choice>
        <xsd:choice>
          <xsd:element ref="bmecat:FVALUE" maxOccurs="unbounded"/>
          <xsd:element ref="bmecat:VALUE_IDREF" maxOccurs="unbounded"/>
        </xsd:choice>
        <xsd:element ref="bmecat:FUNIT" minOccurs="0"/>
        <xsd:element ref="bmecat:FORDER" minOccurs="0"/>
        <xsd:element ref="bmecat:FDESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:FVALUE_DETAILS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:FVALUE_TYPE" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FILE_HASH_VALUE">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="typeSTRING00100">
          <xsd:attribute name="type" use="required">
            <xsd:simpleType>
              <xsd:restriction base="bmecat:dtSTRING">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="50"/>
                <xsd:pattern
                    value="SHA512|SHA384|SHA256|SHA1|MD5|RIPEMD128|RIPEMD160|RIPEMD256|RIPEMD320|Tiger192|Tiger160|Tiger128|HAVAL|PANAMA|WHIRLPOOL|LMHash|NTLM|\w{1,50}"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
          <xsd:attribute name="lang" type="dtLANG" use="optional"/>
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FILE_NAME">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="250"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="FILE_SIZE">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtCOUNT">
        <xsd:minInclusive value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="FINAL_DELIVERY_IDREF" type="bmecat:typePARTY_ID"/>
  <xsd:element name="FINAL_DELIVERY_PARTY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:PARTY"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FINANCE_CHARGE" type="bmecat:dtNUMBER"/>
  <xsd:element name="FINANCE_CHARGE_DESCR">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:restriction base="bmecat:dtMLSTRING">
          <xsd:minLength value="1"/>
          <xsd:maxLength value="300"/>
        </xsd:restriction>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FOREIGN_AMOUNT" type="bmecat:dtNUMBER"/>
  <xsd:element name="FOREIGN_CURRENCY" type="bmecat:dtCURRENCIES"/>
  <xsd:element name="GENERATION_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="GENERATOR_INFO">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="250"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="HEADER_UDX" type="udxHEADER"/>
  <xsd:element name="HOLDER">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="50"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="IL_INVOICE_LIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="IL_INVOICE_LIST_ITEM" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="IL_INVOICE_LIST_ITEM">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="INVOICE_REFERENCE"/>
        <xsd:sequence>
          <xsd:element ref="FOREIGN_CURRENCY" minOccurs="0"/>
          <xsd:element ref="FOREIGN_AMOUNT" minOccurs="0"/>
          <xsd:element ref="EXCHANGE_RATE" minOccurs="0"/>
        </xsd:sequence>
        <xsd:element ref="NET_VALUE_GOODS"/>
        <xsd:element ref="NET_VALUE_EXTRA" minOccurs="0"/>
        <xsd:element ref="ALLOW_OR_CHARGES_FIX" minOccurs="0"/>
        <xsd:element ref="TOTAL_TAX"/>
        <xsd:element ref="TOTAL_AMOUNT"/>
        <xsd:element ref="REWARDS" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="INVOICE_HEADER"/>
        <xsd:element ref="INVOICE_ITEM_LIST"/>
        <xsd:element ref="INVOICE_SUMMARY"/>
      </xsd:sequence>
      <xsd:attribute name="version" type="typeOPENTRANS_version" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_ADJUSTMENT">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ADJUSTED_INVOICE_SUMMARY"/>
        <xsd:element ref="ADJUSTMENT_REASON_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ADJUSTMENT_REASON_CODE" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_CORRECTION">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="INVOICE_REFERENCE" minOccurs="0"/>
        <xsd:element ref="ADJUSTMENT_REASON_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ADJUSTMENT_REASON_CODE" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_COVERAGE" default="single">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="20"/>
        <xsd:enumeration value="single"/>
        <xsd:enumeration value="collective"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="INVOICE_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="INVOICE_DESCR">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:restriction base="bmecat:dtMLSTRING">
          <xsd:minLength value="1"/>
          <xsd:maxLength value="300"/>
        </xsd:restriction>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_HEADER">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="CONTROL_INFO" minOccurs="0"/>
        <xsd:element ref="INVOICE_INFO"/>
        <xsd:element ref="ORDER_HISTORY" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_ID">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="250"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="INVOICE_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="INVOICE_ID"/>
        <xsd:element ref="INVOICE_DATE"/>
        <xsd:element ref="REASON_FOR_TRANSFER" minOccurs="0"/>
        <xsd:element ref="INVOICE_TYPE" minOccurs="0"/>
        <xsd:element ref="INVOICE_COVERAGE" minOccurs="0"/>
        <xsd:element ref="DELIVERYNOTE_ID" minOccurs="0"/>
        <xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
        <xsd:element ref="DELIVERY_IDREF" minOccurs="0"/>
        <xsd:element ref="bmecat:LANGUAGE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:MIME_ROOT" minOccurs="0"/>
        <xsd:element ref="PARTIES"/>
        <xsd:element ref="INVOICE_ISSUER_IDREF"/>
        <xsd:element ref="INVOICE_RECIPIENT_IDREF"/>
        <xsd:element ref="bmecat:BUYER_IDREF" minOccurs="0"/>
        <xsd:element ref="bmecat:SUPPLIER_IDREF" minOccurs="0"/>
        <xsd:element ref="PAYER_IDREF" minOccurs="0"/>
        <xsd:element ref="REMITTEE_IDREF" minOccurs="0"/>
        <xsd:element ref="DOCEXCHANGE_PARTIES_REFERENCE" minOccurs="0"/>
        <xsd:element ref="bmecat:CURRENCY"/>
        <xsd:element ref="PAYMENT" minOccurs="0"/>
        <xsd:element ref="TERMS_AND_CONDITIONS" minOccurs="0"/>
        <xsd:element ref="bmecat:ACCOUNTING_INFO" minOccurs="0"/>
        <xsd:element ref="E_BILLING" minOccurs="0"/>
        <xsd:element ref="LOGISTIC_DETAILS_INFO" minOccurs="0"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="HEADER_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_ISSUER_IDREF" type="bmecat:typePARTY_ID"/>
  <xsd:element name="INVOICE_ITEM">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="LINE_ITEM_ID"/>
        <xsd:element ref="PRODUCT_ID"/>
        <xsd:element ref="PRODUCT_FEATURES" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="PRODUCT_COMPONENTS" minOccurs="0"/>
        <xsd:element ref="QUANTITY"/>
        <xsd:element ref="bmecat:ORDER_UNIT"/>
        <xsd:element ref="PRODUCT_PRICE_FIX"/>
        <xsd:element ref="PRICE_LINE_AMOUNT"/>
        <xsd:sequence>
          <xsd:element ref="FOREIGN_CURRENCY" minOccurs="0"/>
          <xsd:element ref="EXCHANGE_RATE" minOccurs="0"/>
        </xsd:sequence>
        <xsd:element ref="INVOICE_CORRECTION" minOccurs="0"/>
        <xsd:element ref="ORDER_REFERENCE" minOccurs="0"/>
        <xsd:element ref="SUPPLIER_ORDER_REFERENCE" minOccurs="0"/>
        <xsd:element ref="CUSTOMER_ORDER_REFERENCE" minOccurs="0"/>
        <xsd:element ref="DELIVERY_REFERENCE" minOccurs="0"/>
        <xsd:element ref="LOGISTIC_DETAILS" minOccurs="0"/>
        <xsd:element ref="bmecat:ACCOUNTING_INFO" minOccurs="0"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ITEM_UDX" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="type" use="optional">
        <xsd:simpleType>
          <xsd:restriction base="bmecat:dtSTRING">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="20"/>
            <xsd:enumeration value="order_item"/>
            <xsd:enumeration value="extra_item"/>
            <xsd:enumeration value="pledge_item"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_ITEM_LIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="INVOICE_ITEM" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_ORIGINAL">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="MIME"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_PARTY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:PARTY"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_RECIPIENT_IDREF" type="bmecat:typePARTY_ID"/>
  <xsd:element name="INVOICE_REFERENCE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="INVOICE_ID"/>
        <xsd:element ref="LINE_ITEM_ID" minOccurs="0"/>
        <xsd:element ref="INVOICE_DATE" minOccurs="0"/>
        <xsd:element ref="POST_DATE" minOccurs="0"/>
        <xsd:element ref="REASON_FOR_TRANSFER" minOccurs="0"/>
        <xsd:element ref="INVOICE_DESCR" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="type" use="optional" default="invoice">
        <xsd:simpleType>
          <xsd:restriction base="bmecat:dtSTRING">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="20"/>
            <xsd:enumeration value="invoice"/>
            <xsd:enumeration value="invoicelist"/>
            <xsd:enumeration value="credit_memo"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_SUMMARY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="TOTAL_ITEM_NUM"/>
        <xsd:element ref="NET_VALUE_GOODS"/>
        <xsd:element ref="NET_VALUE_EXTRA" minOccurs="0"/>
        <xsd:element ref="TOTAL_AMOUNT"/>
        <xsd:element ref="ALLOW_OR_CHARGES_FIX" minOccurs="0"/>
        <xsd:element ref="TOTAL_TAX"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICE_TYPE" default="invoice">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="20"/>
        <xsd:enumeration value="advice_of_amendment"/>
        <xsd:enumeration value="credit_memo"/>
        <xsd:enumeration value="invoice"/>
        <xsd:enumeration value="invoice_copy"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="INVOICELIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="INVOICELIST_HEADER"/>
        <xsd:element ref="INVOICELIST_ITEM_LIST"/>
        <xsd:element ref="INVOICELIST_SUMMARY"/>
      </xsd:sequence>
      <xsd:attribute name="version" type="typeOPENTRANS_version" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICELIST_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="INVOICELIST_HEADER">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="CONTROL_INFO" minOccurs="0"/>
        <xsd:element ref="INVOICELIST_INFO"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICELIST_ID">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="250"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="INVOICELIST_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="INVOICELIST_ID"/>
        <xsd:element ref="INVOICELIST_DATE"/>
        <xsd:element ref="REASON_FOR_TRANSFER" minOccurs="0"/>
        <xsd:element ref="INVOICELIST_TYPE" minOccurs="0"/>
        <xsd:element ref="ACCOUNTING_PERIOD"/>
        <xsd:element ref="bmecat:LANGUAGE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:MIME_ROOT" minOccurs="0"/>
        <xsd:element ref="PARTIES"/>
        <xsd:element ref="INVOICE_ISSUER_IDREF" minOccurs="0"/>
        <xsd:element ref="INVOICE_RECIPIENT_IDREF" minOccurs="0"/>
        <xsd:element ref="bmecat:BUYER_IDREF" minOccurs="0"/>
        <xsd:element ref="bmecat:SUPPLIER_IDREF" minOccurs="0"/>
        <xsd:element ref="PAYER_IDREF" minOccurs="0"/>
        <xsd:element ref="REMITTEE_IDREF" minOccurs="0"/>
        <xsd:element ref="DOCEXCHANGE_PARTIES_REFERENCE" minOccurs="0"/>
        <xsd:element ref="bmecat:CURRENCY"/>
        <xsd:element ref="PAYMENT" minOccurs="0"/>
        <xsd:element ref="TERMS_AND_CONDITIONS" minOccurs="0"/>
        <xsd:element ref="bmecat:ACCOUNTING_INFO" minOccurs="0"/>
        <xsd:element ref="E_BILLING" minOccurs="0"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="HEADER_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICELIST_ITEM">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="LINE_ITEM_ID"/>
        <xsd:element ref="INVOICE_RECIPIENT_IDREF" minOccurs="0"/>
        <xsd:element ref="DELIVERY_IDREF" minOccurs="0"/>
        <xsd:element ref="INVOICE_ISSUER_IDREF" minOccurs="0"/>
        <xsd:element ref="ACCOUNTING_PERIOD" minOccurs="0"/>
        <xsd:element ref="CREDIT_LIMIT" minOccurs="0"/>
        <xsd:element ref="OPENING_BALANCE" minOccurs="0"/>
        <xsd:element ref="IL_INVOICE_LIST"/>
        <xsd:element ref="TOTAL_ITEM_NUM" minOccurs="0"/>
        <xsd:element ref="NET_VALUE_GOODS"/>
        <xsd:element ref="NET_VALUE_EXTRA" minOccurs="0"/>
        <xsd:element ref="ALLOW_OR_CHARGES_FIX" minOccurs="0"/>
        <xsd:element ref="TOTAL_AMOUNT"/>
        <xsd:element ref="TOTAL_TAX"/>
        <xsd:element ref="DIRECT_DEBIT" minOccurs="0"/>
        <xsd:element ref="CLOSING_BALANCE" minOccurs="0"/>
        <xsd:element ref="AMOUNT_OVER_CREDIT_LIMIT" minOccurs="0"/>
        <xsd:element ref="CREDIT_AVAILABLE" minOccurs="0"/>
        <xsd:element ref="REWARDS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ITEM_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICELIST_ITEM_LIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="INVOICELIST_ITEM" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICELIST_SUMMARY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="TOTAL_ITEM_NUM"/>
        <xsd:element ref="NET_VALUE_GOODS"/>
        <xsd:element ref="TOTAL_AMOUNT"/>
        <xsd:element ref="TOTAL_TAX"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="INVOICELIST_TYPE" default="collective_invoice">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="25"/>
        <xsd:enumeration value="collect_credit_memo"/>
        <xsd:enumeration value="collective_invoice"/>
        <xsd:enumeration value="credit_card_statement"/>
        <xsd:enumeration value="invoice_list"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ITEM_UDX" type="udxITEM"/>
  <xsd:element name="LINE_ITEM_ID">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="50"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="LOGISTIC_DETAILS">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:CUSTOMS_TARIFF_NUMBER" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:STATISTICS_FACTOR" minOccurs="0"/>
        <xsd:element ref="bmecat:COUNTRY_OF_ORIGIN" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:PRODUCT_DIMENSIONS" minOccurs="0"/>
        <xsd:element ref="bmecat:SPECIAL_TREATMENT_CLASS" minOccurs="0"/>
        <xsd:element ref="bmecat:TRANSPORT" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="PACKAGE_INFO" minOccurs="0"/>
        <xsd:element ref="bmecat:MEANS_OF_TRANSPORT" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="LOGISTIC_DETAILS_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:COUNTRY_OF_ORIGIN" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:TRANSPORT" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="PACKAGE_INFO" minOccurs="0"/>
        <xsd:element ref="bmecat:MEANS_OF_TRANSPORT" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="LOT_NUMBER">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="80"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="MANUFACTURER_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:MANUFACTURER_IDREF"/>
        <xsd:element ref="bmecat:MANUFACTURER_PID"/>
        <xsd:element ref="bmecat:MANUFACTURER_TYPE_DESCR" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MEANS_OF_TRANSPORT_IDREF">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="50"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="MIME">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:MIME_TYPE" minOccurs="0"/>
        <xsd:choice>
          <xsd:sequence>
            <xsd:element ref="bmecat:MIME_SOURCE" maxOccurs="unbounded"/>
            <xsd:element ref="FILE_HASH_VALUE" minOccurs="0" maxOccurs="unbounded"/>
          </xsd:sequence>
          <xsd:element ref="MIME_EMBEDDED" maxOccurs="unbounded"/>
        </xsd:choice>
        <xsd:element ref="bmecat:MIME_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:MIME_ALT" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MIME_PURPOSE" minOccurs="0"/>
        <xsd:element ref="bmecat:MIME_ORDER" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MIME_DATA" type="xmime:base64Binary"/>
  <xsd:element name="MIME_EMBEDDED">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="MIME_DATA"/>
        <xsd:element ref="FILE_NAME" minOccurs="0"/>
        <xsd:element ref="FILE_SIZE" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="lang" type="dtLANG" use="optional"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MIME_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="MIME" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="MIME_PURPOSE">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="20"/>
        <xsd:enumeration value="conformity"/>
        <xsd:enumeration value="data_sheet"/>
        <xsd:enumeration value="detail"/>
        <xsd:enumeration value="directions"/>
        <xsd:enumeration value="fax_image"/>
        <xsd:enumeration value="freehand_sketch"/>
        <xsd:enumeration value="icon"/>
        <xsd:enumeration value="logo"/>
        <xsd:enumeration value="manual"/>
        <xsd:enumeration value="mounting_guidelines"/>
        <xsd:enumeration value="normal"/>
        <xsd:enumeration value="original_document"/>
        <xsd:enumeration value="repair_manual"/>
        <xsd:enumeration value="safety_data_sheet"/>
        <xsd:enumeration value="signatur_file"/>
        <xsd:enumeration value="service_descr"/>
        <xsd:enumeration value="service_record"/>
        <xsd:enumeration value="thumbnail"/>
        <xsd:enumeration value="verification_report"/>
        <xsd:enumeration value="warranty"/>
        <xsd:enumeration value="others"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="NET_VALUE_EXTRA" type="bmecat:dtNUMBER"/>
  <xsd:element name="NET_VALUE_GOODS" type="bmecat:dtNUMBER"/>
  <xsd:element name="OPENING_BALANCE" type="bmecat:dtNUMBER"/>
  <xsd:element name="ORDER">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDER_HEADER"/>
        <xsd:element ref="ORDER_ITEM_LIST"/>
        <xsd:element ref="ORDER_SUMMARY"/>
      </xsd:sequence>
      <xsd:attribute name="version" type="typeOPENTRANS_version" use="required"/>
      <xsd:attribute name="type" use="required">
        <xsd:simpleType>
          <xsd:restriction base="bmecat:dtSTRING">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="20"/>
            <xsd:enumeration value="standard"/>
            <xsd:enumeration value="express"/>
            <xsd:enumeration value="release"/>
            <xsd:enumeration value="consignment"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDER_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="ORDER_DESCR">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:restriction base="bmecat:dtMLSTRING">
          <xsd:minLength value="1"/>
          <xsd:maxLength value="300"/>
        </xsd:restriction>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDER_HEADER">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="CONTROL_INFO" minOccurs="0"/>
        <xsd:element ref="SOURCING_INFO" minOccurs="0"/>
        <xsd:element ref="ORDER_INFO"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDER_HISTORY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDER_ID"/>
        <xsd:element ref="ALT_CUSTOMER_ORDER_ID" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="SUPPLIER_ORDER_ID" minOccurs="0"/>
        <xsd:element ref="ORDER_DATE" minOccurs="0"/>
        <xsd:element ref="ORDER_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="DELIVERYNOTE_ID" minOccurs="0"/>
        <xsd:element ref="DELIVERYNOTE_DATE" minOccurs="0"/>
        <xsd:element ref="AGREEMENT" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="CATALOG_REFERENCE" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDER_ID">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="250"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="ORDER_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDER_ID"/>
        <xsd:element ref="ORDER_DATE"/>
        <xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
        <xsd:element ref="bmecat:LANGUAGE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:MIME_ROOT" minOccurs="0"/>
        <xsd:element ref="PARTIES"/>
        <xsd:element ref="CUSTOMER_ORDER_REFERENCE" minOccurs="0"/>
        <xsd:element ref="ORDER_PARTIES_REFERENCE"/>
        <xsd:element ref="DOCEXCHANGE_PARTIES_REFERENCE" minOccurs="0"/>
        <xsd:element ref="bmecat:CURRENCY" minOccurs="0"/>
        <xsd:element ref="PAYMENT" minOccurs="0"/>
        <xsd:element ref="TERMS_AND_CONDITIONS" minOccurs="0"/>
        <xsd:element ref="PARTIAL_SHIPMENT_ALLOWED" minOccurs="0"/>
        <xsd:element ref="bmecat:TRANSPORT" minOccurs="0"/>
        <xsd:element ref="bmecat:INTERNATIONAL_RESTRICTIONS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="HEADER_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDER_ITEM">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="LINE_ITEM_ID"/>
        <xsd:element ref="PRODUCT_ID"/>
        <xsd:element ref="PRODUCT_FEATURES" minOccurs="0"/>
        <xsd:element ref="PRODUCT_COMPONENTS" minOccurs="0"/>
        <xsd:element ref="QUANTITY"/>
        <xsd:element ref="bmecat:ORDER_UNIT"/>
        <xsd:element ref="PRODUCT_PRICE_FIX" minOccurs="0"/>
        <xsd:element ref="PRICE_LINE_AMOUNT" minOccurs="0"/>
        <xsd:choice minOccurs="0">
          <xsd:sequence>
            <xsd:element ref="PARTIAL_SHIPMENT_ALLOWED" minOccurs="0"/>
            <xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
          </xsd:sequence>
          <xsd:element ref="PARTIAL_DELIVERY_LIST" minOccurs="0"/>
        </xsd:choice>
        <xsd:element ref="SOURCING_INFO" minOccurs="0"/>
        <xsd:element ref="CUSTOMER_ORDER_REFERENCE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:ACCOUNTING_INFO" minOccurs="0"/>
        <xsd:element ref="SHIPMENT_PARTIES_REFERENCE" minOccurs="0"/>
        <xsd:element ref="bmecat:TRANSPORT" minOccurs="0"/>
        <xsd:element ref="bmecat:INTERNATIONAL_RESTRICTIONS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:SPECIAL_TREATMENT_CLASS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ITEM_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDER_ITEM_LIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDER_ITEM" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDER_PARTIES">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="BUYER_PARTY"/>
        <xsd:element ref="SUPPLIER_PARTY"/>
        <xsd:element ref="INVOICE_PARTY" minOccurs="0"/>
        <xsd:element ref="SHIPMENT_PARTIES" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDER_PARTIES_REFERENCE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:BUYER_IDREF"/>
        <xsd:element ref="bmecat:SUPPLIER_IDREF"/>
        <xsd:element ref="INVOICE_RECIPIENT_IDREF" minOccurs="0"/>
        <xsd:element ref="SHIPMENT_PARTIES_REFERENCE" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDER_REFERENCE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDER_ID"/>
        <xsd:element ref="LINE_ITEM_ID"/>
        <xsd:element ref="ORDER_DATE" minOccurs="0"/>
        <xsd:element ref="ORDER_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="AGREEMENT" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="CATALOG_REFERENCE" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDER_SUMMARY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="TOTAL_ITEM_NUM"/>
        <xsd:element ref="TOTAL_AMOUNT" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERCHANGE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDERCHANGE_HEADER"/>
        <xsd:element ref="ORDERCHANGE_ITEM_LIST"/>
        <xsd:element ref="ORDERCHANGE_SUMMARY"/>
      </xsd:sequence>
      <xsd:attribute name="version" type="typeOPENTRANS_version" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERCHANGE_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="ORDERCHANGE_HEADER">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="CONTROL_INFO" minOccurs="0"/>
        <xsd:element ref="ORDERCHANGE_INFO"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERCHANGE_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDER_ID"/>
        <xsd:element ref="ORDERCHANGE_DATE"/>
        <xsd:element ref="ORDER_DATE" minOccurs="0"/>
        <xsd:element ref="ALT_CUSTOMER_ORDER_ID" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="SUPPLIER_ORDER_ID" minOccurs="0"/>
        <xsd:element ref="ORDERCHANGE_SEQUENCE_ID"/>
        <xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
        <xsd:element ref="bmecat:LANGUAGE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:MIME_ROOT" minOccurs="0"/>
        <xsd:element ref="PARTIES"/>
        <xsd:element ref="ORDER_PARTIES_REFERENCE"/>
        <xsd:element ref="DOCEXCHANGE_PARTIES_REFERENCE" minOccurs="0"/>
        <xsd:element ref="bmecat:CURRENCY" minOccurs="0"/>
        <xsd:element ref="PAYMENT" minOccurs="0"/>
        <xsd:element ref="TERMS_AND_CONDITIONS" minOccurs="0"/>
        <xsd:element ref="PARTIAL_SHIPMENT_ALLOWED" minOccurs="0"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="HEADER_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERCHANGE_ITEM_LIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDER_ITEM" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERCHANGE_SEQUENCE_ID" type="bmecat:dtCOUNT"/>
  <xsd:element name="ORDERCHANGE_SUMMARY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="TOTAL_ITEM_NUM"/>
        <xsd:element ref="TOTAL_AMOUNT" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERRESPONSE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDERRESPONSE_HEADER"/>
        <xsd:element ref="ORDERRESPONSE_ITEM_LIST"/>
        <xsd:element ref="ORDERRESPONSE_SUMMARY"/>
      </xsd:sequence>
      <xsd:attribute name="version" type="typeOPENTRANS_version" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERRESPONSE_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="ORDERRESPONSE_HEADER">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="CONTROL_INFO" minOccurs="0"/>
        <xsd:element ref="ORDERRESPONSE_INFO"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERRESPONSE_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDER_ID"/>
        <xsd:element ref="ORDERRESPONSE_DATE"/>
        <xsd:element ref="ORDER_DATE" minOccurs="0"/>
        <xsd:element ref="ALT_CUSTOMER_ORDER_ID" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="SUPPLIER_ORDER_ID" minOccurs="0"/>
        <xsd:element ref="ORDERCHANGE_SEQUENCE_ID" minOccurs="0"/>
        <xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
        <xsd:element ref="bmecat:LANGUAGE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:MIME_ROOT" minOccurs="0"/>
        <xsd:element ref="PARTIES"/>
        <xsd:element ref="ORDER_PARTIES_REFERENCE"/>
        <xsd:element ref="DOCEXCHANGE_PARTIES_REFERENCE" minOccurs="0"/>
        <xsd:element ref="bmecat:CURRENCY" minOccurs="0"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="HEADER_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERRESPONSE_ITEM">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="LINE_ITEM_ID"/>
        <xsd:element ref="PRODUCT_ID"/>
        <xsd:element ref="PRODUCT_FEATURES" minOccurs="0"/>
        <xsd:element ref="PRODUCT_COMPONENTS" minOccurs="0"/>
        <xsd:element ref="QUANTITY"/>
        <xsd:element ref="bmecat:ORDER_UNIT"/>
        <xsd:element ref="PRODUCT_PRICE_FIX" minOccurs="0"/>
        <xsd:element ref="PRICE_LINE_AMOUNT" minOccurs="0"/>
        <xsd:choice minOccurs="0">
          <xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
          <xsd:element ref="PARTIAL_DELIVERY_LIST" minOccurs="0"/>
        </xsd:choice>
        <xsd:element ref="SHIPMENT_PARTIES_REFERENCE" minOccurs="0"/>
        <xsd:element ref="bmecat:SPECIAL_TREATMENT_CLASS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ITEM_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERRESPONSE_ITEM_LIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="ORDERRESPONSE_ITEM" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORDERRESPONSE_SUMMARY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="TOTAL_ITEM_NUM"/>
        <xsd:element ref="TOTAL_AMOUNT" minOccurs="0"/>
        <xsd:element ref="ALLOW_OR_CHARGES_FIX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORIGINAL_INVOICE_SUMMARY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="NET_VALUE_GOODS"/>
        <xsd:element ref="TOTAL_TAX"/>
        <xsd:element ref="TOTAL_AMOUNT"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ORIGINAL_SUMMARY_AMOUNT" type="bmecat:dtNUMBER"/>
  <xsd:element name="PACKAGE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="PACKAGE_ID" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="PACKAGE_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:PACKING_UNIT_CODE" minOccurs="0"/>
        <xsd:element ref="bmecat:PACKING_UNIT_DESCR" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="PACKAGE_ORDER_UNIT_QUANTITY" minOccurs="0"/>
        <xsd:element ref="PACKAGE_QUANTITY" minOccurs="0"/>
        <xsd:element ref="PACKAGE_DIMENSIONS" minOccurs="0"/>
        <xsd:element ref="MEANS_OF_TRANSPORT_IDREF" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="SUB_PACKAGES" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PACKAGE_DESCR">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:restriction base="bmecat:dtMLSTRING">
          <xsd:minLength value="1"/>
          <xsd:maxLength value="300"/>
        </xsd:restriction>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PACKAGE_DIMENSIONS">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:VOLUME" minOccurs="0"/>
        <xsd:element ref="bmecat:WEIGHT" minOccurs="0"/>
        <xsd:element ref="bmecat:LENGTH" minOccurs="0"/>
        <xsd:element ref="bmecat:WIDTH" minOccurs="0"/>
        <xsd:element ref="bmecat:DEPTH" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PACKAGE_ID">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="typeSTRING00050">
          <xsd:attribute name="type" use="optional">
            <xsd:simpleType>
              <xsd:restriction base="bmecat:dtSTRING">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="50"/>
                <xsd:pattern value="buyer_specific|deliverer_specific|grai|mtv|nve|sscc|supplier_specific|\w{1,50}"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PACKAGE_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="PACKAGE" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PACKAGE_ORDER_UNIT_QUANTITY">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtNUMBER">
        <xsd:minInclusive value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PACKAGE_QUANTITY">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtNUMBER">
        <xsd:minInclusive value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PACKAGE_TYPE">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="50"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PARTIAL_DELIVERY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="QUANTITY"/>
        <xsd:element ref="DELIVERY_DATE" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PARTIAL_DELIVERY_LIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="PARTIAL_DELIVERY" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PARTIAL_SHIPMENT" type="bmecat:dtBOOLEAN"/>
  <xsd:element name="PARTIAL_SHIPMENT_ALLOWED" type="bmecat:dtBOOLEAN"/>
  <xsd:element name="PARTIES">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="PARTY" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PARTY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:PARTY_ID" maxOccurs="unbounded"/>
        <xsd:element ref="PARTY_ROLE" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ADDRESS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ACCOUNT" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PARTY_ROLE">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="20"/>
        <xsd:enumeration value="buyer"/>
        <xsd:enumeration value="central_regulator"/>
        <xsd:enumeration value="customer"/>
        <xsd:enumeration value="deliverer"/>
        <xsd:enumeration value="delivery"/>
        <xsd:enumeration value="document_creator"/>
        <xsd:enumeration value="final_delivery"/>
        <xsd:enumeration value="intermediary"/>
        <xsd:enumeration value="invoice_issuer"/>
        <xsd:enumeration value="invoice_recipient"/>
        <xsd:enumeration value="ipp_operator"/>
        <xsd:enumeration value="manufacturer"/>
        <xsd:enumeration value="marketplace"/>
        <xsd:enumeration value="payer"/>
        <xsd:enumeration value="remittee"/>
        <xsd:enumeration value="standardization_body"/>
        <xsd:enumeration value="supplier"/>
        <xsd:enumeration value="trustedthirdparty"/>
        <xsd:enumeration value="other"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="PAYER_IDREF" type="bmecat:typePARTY_ID"/>
  <xsd:element name="PAYMENT">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:choice>
          <xsd:element ref="CARD"/>
          <xsd:element ref="ACCOUNT" maxOccurs="unbounded"/>
          <xsd:element ref="DEBIT"/>
          <xsd:element ref="CHECK"/>
          <xsd:element ref="CASH"/>
        </xsd:choice>
        <xsd:element ref="CENTRAL_REGULATION" minOccurs="0"/>
        <xsd:element ref="PAYMENT_TERMS" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PAYMENT_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="PAYMENT_TERM">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="typePAYMENT_TERM">
          <xsd:attribute name="type" use="required">
            <xsd:simpleType>
              <xsd:restriction base="bmecat:dtSTRING">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="20"/>
                <xsd:enumeration value="unece"/>
                <xsd:enumeration value="other"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="PAYMENT_TERMS">
    <xsd:complexType>
      <xsd:sequence minOccurs="0">
        <xsd:element ref="PAYMENT_TERM" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="TIME_FOR_PAYMENT" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="VALUE_DATE" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="POST_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="PRICE_BASE_FIX">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="PRICE_UNIT_VALUE"/>
        <xsd:element ref="bmecat:PRICE_UNIT"/>
        <xsd:element ref="bmecat:PRICE_UNIT_FACTOR" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
