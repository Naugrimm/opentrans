      <xsd:simpleContent>
        <xsd:extension base="typeMLSTRING64000">
          <xsd:attribute name="type" use="optional">
            <xsd:simpleType>
              <xsd:restriction base="bmecat:dtSTRING">
                <xsd:minLength value="1"/>
                <xsd:maxLength value="250"/>
