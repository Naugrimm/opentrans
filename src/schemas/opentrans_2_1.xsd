        <xsd:element ref="bmecat:SPECIAL_TREATMENT_CLASS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="MIME_INFO" minOccurs="0"/>
        <xsd:element ref="REMARKS" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ITEM_UDX" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="RFQ_ITEM_LIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="RFQ_ITEM" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="RFQ_SUMMARY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="TOTAL_ITEM_NUM"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SERIAL_NUMBER">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="80"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SHIPMENT_ID">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="250"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SHIPMENT_PARTIES">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DELIVERY_PARTY"/>
        <xsd:element ref="FINAL_DELIVERY_PARTY" minOccurs="0"/>
        <xsd:element ref="TRANSPORT_PARTY" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SHIPMENT_PARTIES_REFERENCE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="DELIVERY_IDREF"/>
        <xsd:element ref="FINAL_DELIVERY_IDREF" minOccurs="0"/>
        <xsd:element ref="DELIVERER_IDREF" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SIGNATURE">
    <xsd:complexType>
      <xsd:choice>
        <xsd:element ref="MIME"/>
        <xsd:element ref="SIGNATURE_IN_ORIGINAL"/>
        <xsd:element ref="XML_SIGNATURE"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SIGNATURE_AND_VERIFICATION">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="SIGNATURE"/>
        <xsd:element ref="VERIFICATION" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SIGNATURE_IN_ORIGINAL" type="bmecat:dtBOOLEAN"/>
  <xsd:element name="SOURCING_INFO">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="QUOTATION_ID" minOccurs="0"/>
        <xsd:element ref="AGREEMENT" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="CATALOG_REFERENCE" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="STOP_AUTOMATIC_PROCESSING">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="250"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SUB_PACKAGES">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="PACKAGE" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SUPPLIER_ORDER_ID">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="250"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SUPPLIER_ORDER_ITEM_ID">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="50"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="SUPPLIER_ORDER_REFERENCE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="SUPPLIER_ORDER_ID"/>
        <xsd:element ref="SUPPLIER_ORDER_ITEM_ID"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SUPPLIER_PARTY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:PARTY"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TAX_AMOUNT" type="bmecat:dtNUMBER"/>
  <xsd:element name="TAX_BASE" type="bmecat:dtNUMBER"/>
  <xsd:element name="TAX_DETAILS_FIX">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:CALCULATION_SEQUENCE" minOccurs="0"/>
        <xsd:element ref="bmecat:TAX_CATEGORY" minOccurs="0"/>
        <xsd:element ref="bmecat:TAX_TYPE" minOccurs="0"/>
        <xsd:element ref="bmecat:TAX" minOccurs="0"/>
        <xsd:element ref="TAX_AMOUNT" minOccurs="0"/>
        <xsd:element ref="TAX_BASE" minOccurs="0"/>
        <xsd:element ref="bmecat:EXEMPTION_REASON" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="bmecat:JURISDICTION" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TAX_NUMBER">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="50"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="TERMS_AND_CONDITIONS">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:maxLength value="250"/>
        <xsd:minLength value="1"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="TIME_FOR_PAYMENT">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:choice>
          <xsd:element ref="PAYMENT_DATE"/>
          <xsd:element ref="DAYS"/>
        </xsd:choice>
        <xsd:choice minOccurs="0">
          <xsd:element ref="DISCOUNT_FACTOR" minOccurs="0"/>
          <xsd:element ref="ALLOW_OR_CHARGES_FIX" minOccurs="0"/>
        </xsd:choice>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TOTAL_AMOUNT" type="bmecat:dtNUMBER"/>
  <xsd:element name="TOTAL_ITEM_NUM" type="bmecat:dtCOUNT"/>
  <xsd:element name="TOTAL_TAX">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="TAX_DETAILS_FIX" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="TOTAL_TAX_AMOUNT" type="bmecat:dtNUMBER"/>
  <xsd:element name="TRACKING_TRACING_URL">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="255"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="TRANSPORT_PARTY">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bmecat:PARTY"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="VALUE_DATE" type="bmecat:dtDATETIME"/>
  <xsd:element name="VERIFICATION">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="VERIFICATION_PARTY_IDREF"/>
        <xsd:element ref="VERIFICATION_SUCCESS"/>
        <xsd:element ref="VERIFICATION_REPORT"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="VERIFICATION_ATTACHMENT">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="MIME"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="VERIFICATION_PARTY_IDREF" type="bmecat:typePARTY_ID"/>
  <xsd:element name="VERIFICATION_PROTOCOL">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="RESULT_CODE"/>
        <xsd:element ref="RESULT_DESCR" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="VERIFICATION_REPORT">
    <xsd:complexType>
      <xsd:choice>
        <xsd:element ref="VERIFICATION_ATTACHMENT"/>
        <xsd:element ref="VERIFICATION_PROTOCOL"/>
        <xsd:element ref="VERIFICATION_XMLREPORT"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="VERIFICATION_RESULT" type="bmecat:dtBOOLEAN"/>
  <xsd:element name="VERIFICATION_SUCCESS" type="bmecat:dtBOOLEAN"/>
  <xsd:element name="VERIFICATION_XMLREPORT">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="XML_FORMAT"/>
        <xsd:element ref="REPORT_UDX"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="XML_FORMAT">
    <xsd:simpleType>
      <xsd:restriction base="bmecat:dtSTRING">
        <xsd:minLength value="1"/>
        <xsd:maxLength value="50"/>
      </xsd:restriction>
    </xsd:simpleType>
  </xsd:element>
  <xsd:element name="XML_SIGNATURE">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="xsig:Signature"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
